
<% flash.each do |key, value| %>
    <%= content_tag(:div, value, class: "alert alert-#{key}") %>
<% end %>
<%= form_for(:session, url: sessions_path,:html => {:class => "form-horizontal"})  do |f| %>

    <%= f.label :email %>
    <%= f.text_field :email,{:class => "input-medium", :id => "firsttxt"} %>

    <%= f.label :password %>
    <%= f.password_field :password,{:class => "input-medium"} %>
    <br/>
    <%= f.submit "Sign in",id: "modal_but", class: "btn btn-large btn-success center" %>
<% end %>

