<% provide(:title, @category.name) %>
<div class="row">
  <aside class="span2">
    <%= render 'layouts/side_nav' %>
  </aside>



  <div class="span10 opaque">



    <h1>Listing Resources</h1>

    <div class="resource_content">
      <div id="res_header">
        <h5><%= sort_link @search, :title, "Title" %></h5>
        <h5>Source</h5>
        <h5>Category</h5>
        <h5>Level</h5>
        <h5>Year</h5>
        <h5>Cost</h5>
        <h5> Medium</h5>
        <h5> Rating</h5>
        <h5> Description</h5></div>


      <div class="resources">

        <div>

          <%= render :partial => 'resource' %>


          <%= will_paginate @resources %>
          <%= nether(@resources.total_pages, category_path, ".resources") %>
        </div>
      </div>

    </div>



  </div>









</div>
